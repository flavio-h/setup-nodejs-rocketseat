{
  "name": "node-setup",
  "version": "1.0.0",
  "main": "src/server.ts",
  "author": "flavio-h <flavio.p3.2012@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --respawn --no-notify --ignore-watch node_modules src/server.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.2",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}